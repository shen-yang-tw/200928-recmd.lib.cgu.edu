    <!-- Breadcrumb & User -->
    <div class="px-2 mb-2 justify-between uk-grid uk-grid-small uk-grid-match uk-flex-middle" uk-grid
      uk-height-match="target: > div">
      <!-- Child <div> Default to class="uk-width-auto" -->
